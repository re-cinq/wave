{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "function_name",
    "signature",
    "location",
    "testing"
  ],
  "properties": {
    "function_name": {
      "type": "string"
    },
    "signature": {
      "type": "object",
      "properties": {
        "parameters": {
          "type": "array",
          "items": {
            "type": "object"
          }
        },
        "return_type": {
          "type": "string"
        },
        "visibility": {
          "type": "string"
        }
      }
    },
    "location": {
      "type": "object",
      "properties": {
        "file_path": {
          "type": "string"
        },
        "package": {
          "type": "string"
        },
        "module": {
          "type": "string"
        }
      }
    },
    "testing": {
      "type": "object",
      "properties": {
        "test_file_path": {
          "type": "string"
        },
        "test_cases": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "assertions": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "integration": {
      "type": "object",
      "properties": {
        "exports": {
          "type": "boolean"
        },
        "imports": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "usage_examples": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}