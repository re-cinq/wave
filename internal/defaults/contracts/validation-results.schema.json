{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "validation_results",
    "coverage_assessment",
    "feasibility_check",
    "risk_analysis",
    "recommendations"
  ],
  "properties": {
    "validation_results": {
      "type": "object",
      "properties": {
        "overall_score": {
          "type": "number"
        },
        "completeness": {
          "type": "number"
        },
        "feasibility": {
          "type": "number"
        },
        "impact": {
          "type": "number"
        }
      }
    },
    "coverage_assessment": {
      "type": "object",
      "properties": {
        "areas_covered": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "gaps_identified": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "coverage_percentage": {
          "type": "number"
        }
      }
    },
    "feasibility_check": {
      "type": "object",
      "properties": {
        "resource_requirements": {
          "type": "string"
        },
        "technical_constraints": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "timeline_realistic": {
          "type": "boolean"
        }
      }
    },
    "risk_analysis": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "risk": {
            "type": "string"
          },
          "probability": {
            "type": "string"
          },
          "impact": {
            "type": "string"
          },
          "mitigation": {
            "type": "string"
          }
        }
      }
    },
    "recommendations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "recommendation": {
            "type": "string"
          },
          "rationale": {
            "type": "string"
          },
          "priority": {
            "type": "string"
          }
        }
      }
    }
  }
}