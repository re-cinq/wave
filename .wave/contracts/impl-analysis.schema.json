{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Analysis of actual implementation features and behaviors",
  "required": [
    "implemented_features",
    "cli_commands",
    "config_options"
  ],
  "properties": {
    "implemented_features": {
      "type": "array",
      "description": "List of features implemented in the codebase",
      "items": {
        "type": "object",
        "required": [
          "name",
          "package",
          "file_path"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Feature name or identifier"
          },
          "package": {
            "type": "string",
            "description": "Go package containing the feature"
          },
          "file_path": {
            "type": "string",
            "description": "Path to the implementation file"
          },
          "actual_behavior": {
            "type": "string",
            "description": "Description of actual behavior"
          }
        }
      }
    },
    "cli_commands": {
      "type": "array",
      "description": "Implemented CLI commands",
      "items": {
        "type": "object",
        "required": [
          "name",
          "file_path"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Command name"
          },
          "file_path": {
            "type": "string",
            "description": "Path to command implementation"
          },
          "options": {
            "type": "array",
            "description": "Actually implemented options",
            "items": {
              "type": "object",
              "required": [
                "flag"
              ],
              "properties": {
                "flag": {
                  "type": "string",
                  "description": "Option flag"
                },
                "type": {
                  "type": "string",
                  "description": "Option value type"
                }
              }
            }
          },
          "actual_behavior": {
            "type": "string",
            "description": "Actual implemented behavior"
          }
        }
      }
    },
    "config_options": {
      "type": "array",
      "description": "Actually parsed configuration options",
      "items": {
        "type": "object",
        "required": [
          "path",
          "actual_type"
        ],
        "properties": {
          "path": {
            "type": "string",
            "description": "Config path"
          },
          "actual_type": {
            "type": "string",
            "description": "Actual parsed type"
          },
          "struct_field": {
            "type": "string",
            "description": "Go struct field name"
          }
        }
      }
    },
    "persona_capabilities": {
      "type": "array",
      "description": "Actual persona implementations",
      "items": {
        "type": "object",
        "required": [
          "persona",
          "file_path"
        ],
        "properties": {
          "persona": {
            "type": "string",
            "description": "Persona name"
          },
          "file_path": {
            "type": "string",
            "description": "Path to persona definition"
          },
          "capabilities": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of actual capabilities"
          }
        }
      }
    },
    "contract_types": {
      "type": "array",
      "description": "Implemented contract validators",
      "items": {
        "type": "object",
        "required": [
          "type",
          "file_path"
        ],
        "properties": {
          "type": {
            "type": "string",
            "description": "Contract type name"
          },
          "file_path": {
            "type": "string",
            "description": "Path to validator implementation"
          },
          "actual_behavior": {
            "type": "string",
            "description": "Actual validation behavior"
          }
        }
      }
    }
  }
}